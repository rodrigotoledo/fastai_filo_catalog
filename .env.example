DATABASE_URL=postgresql://photo_finder_user:photo_finder_pass@db:5432/photo_finder
POSTGRES_DB=photo_finder
POSTGRES_USER=photo_finder_user
POSTGRES_PASSWORD=photo_finder_pass
REDIS_URL=redis://redis:6379

# AI Model Configuration
# Opções: "local", "openai", "anthropic", "gemini"
AI_MODEL_TYPE=local

# Para modelos locais (HuggingFace) - SEM CHAVE NECESSÁRIA
LOCAL_MODEL=gpt2  # Opções: gpt2, microsoft/DialoGPT-medium, etc.

# Para OpenAI GPT-4 (requer chave paga - mais quota)
OPENAI_API_KEY=your_openai_api_key_here

# Para Anthropic Claude (requer chave paga - boa quota)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Para Gemini (já tem quota limitada gratuita)
GEMINI_API_KEY=your_gemini_api_key_here
